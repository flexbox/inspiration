{
  "name": "inspiration",
  "description": "Popular quotes from humans working in the web industry",
  "author": "David Leuliette <dleuliette@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdir -p public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose",
    "debug": "node --inspect-brk --no-lazy node_modules/gatsby/dist/bin/gatsby develop",
    "cache:clear": "rm -rf node_modules && rm -rf .cache"
  },
  "dependencies": {
    "autoprefixer": "^10.3.4",
    "babel-plugin-styled-components": "^1.7.1",
    "dotenv": "^6.0.0",
