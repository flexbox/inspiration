    "gatsby-image": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-react-svg": "^2.0.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-webpack-size": "^0.0.2",
    "gatsby-source-contentful": "^2.1.73",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.1",
    "gatsby-transformer-sharp": "^2.1.1",
    "postcss": "^8.3.6",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-pose": "^3.3.0",
    "rebass": "^4.0.7",
    "styled-components": "^4.0.0-beta.7",
    "tailwindcss": "^2.2.15"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "chrome-launcher": "^0.10.2",
    "directory-named-webpack-plugin": "4.0.1",
    "eslint": "^5.6.0",
